generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id                      String   @id @default(uuid())
  fullName                String?
  fatherName              String?
  epicId                  String?
  mobileNumber            String   @unique
  legislativeConstituency String?
  boothNameOrNumber       String?
  gender                  Gender?
  age                     Int?
  email                   String?  @unique  
  @@map("user")
}

enum Gender {
  MALE
  FEMALE
  OTHER
}


